{
  "name": "sd-server",
  "version": "1.0.0",
  "description": "Simple-Docfy Server",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha specs/** --require specs/helpers/bootstrap.js --reporter spec",
    "debug": "node-debug ./bin/www --debug-brk",
    "debug-log": "DEBUG=sd-server:server,express* node-debug ./bin/www --debug-brk",
    "debug-unit-tests": "mocha specs/** --require specs/helpers/bootstrap.js --reporter spec --debug-brk",
    "test-coverage": "istanbul cover _mocha specs/** --require specs/helpers/bootstrap.js --reporter spec",
    "pm2": "pm2 start ./bin/www"
  },
  "author": "Rafael Carneiro de Castro",
  "repository": "castro150/to-def",
  "license": "ISC",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "google-contacts": "^0.1.5",
    "googleapis": "^19.0.0",
    "mongoose": "^4.8.6",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "properties-reader": "0.0.15",
    "winston": "^2.3.1",
    "xml-writer": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "node-mocks-http": "^1.6.2",
    "sinon": "^2.3.2"
  }
}
